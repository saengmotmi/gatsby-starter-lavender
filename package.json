{
  "name": "gatsby-starter-lavender",
  "private": true,
  "version": "2.0.0",
  "description": "Static blog migrated to React Router v7 Framework Mode + Vite",
  "author": "Changhui Lee <iam@xo.dev>",
  "license": "0BSD",
  "type": "module",
  "scripts": {
    "generate:content": "node ./scripts/generate-content.mjs",
    "dev": "node ./scripts/generate-content.mjs && react-router dev",
    "build": "node ./scripts/generate-content.mjs && react-router build",
    "typecheck": "node ./scripts/generate-content.mjs && react-router typegen && tsc",
    "verify": "node ./scripts/generate-content.mjs && react-router typegen && tsc && node ./scripts/generate-content.mjs && react-router build",
    "lint-staged": "lint-staged",
    "husky:install": "husky",
    "postinstall": "node ./scripts/setup-husky.mjs"
  },
  "dependencies": {
    "@react-router/node": "7.13.0",
    "@vanilla-extract/css": "^1.18.0",
    "@vanilla-extract/dynamic": "^2.1.5",
    "gray-matter": "^4.0.3",
    "isbot": "^5",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "7.13.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-prism-plus": "^2.0.1",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "remark-rehype": "^11.1.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@react-router/dev": "7.13.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "yarn@4.12.0",
  "engines": {
    "node": ">=24.0.0 <25",
    "yarn": ">=4.0.0"
  }
}
